Cell["Fluctuations Formula in General", "Section",
 CellChangeTimes->{{3.747395033245633*^9, 3.747395044925301*^9}}],

Cell["\<\
This section now generalizes how the fluctuations scale as a function of \
subsystem size and given filling (number of atoms:: Nt, in total system size \
of L)\
\>", "Text",
 CellChangeTimes->{{3.747395077550167*^9, 3.747395113542226*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"pnlnN", "[", 
   RowBox[{"n_", ",", "Nt_", ",", "l_", ",", "L_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"Nt", "-", "n"}], ")"}], "+", 
        RowBox[{"(", 
         RowBox[{"L", "-", "l"}], ")"}], "-", "1"}], ")"}], "!"}], 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Nt", "-", "n"}], ")"}], "!"}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"L", "-", "l"}], ")"}], "-", "1"}], ")"}], "!"}]}]], ")"}], 
   " ", 
   RowBox[{
    RowBox[{"(", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"n", "+", 
         RowBox[{"(", "l", ")"}], "-", "1"}], ")"}], "!"}], 
      RowBox[{
       RowBox[{"n", "!"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", "l", ")"}], "-", "1"}], ")"}], "!"}]}]], ")"}], "/", 
    RowBox[{"(", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Nt", "+", "L", "-", "1"}], ")"}], "!"}], 
      RowBox[{
       RowBox[{"Nt", "!"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"L", "-", "1"}], ")"}], "!"}]}]], 
     ")"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pnFN", "[", 
   RowBox[{"l_", ",", "L_", ",", "NE_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"pnlnN", "[", 
       RowBox[{"n", ",", "NE", ",", "l", ",", "L"}], "]"}], 
      SuperscriptBox["n", "2"]}], ",", 
     RowBox[{"{", 
      RowBox[{"n", ",", "0", ",", "NE"}], "}"}]}], "]"}], "-", 
   SuperscriptBox[
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"pnlnN", "[", 
        RowBox[{"n", ",", "NE", ",", "l", ",", "L"}], "]"}], "n"}], ",", 
      RowBox[{"{", 
       RowBox[{"n", ",", "0", ",", "NE"}], "}"}]}], "]"}], 
    "2"]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"FullSimplify", "[", 
   RowBox[{"pnFN", "[", 
    RowBox[{"l", ",", "L", ",", "n"}], "]"}], "]"}], " ", 
  RowBox[{"(*", 
   RowBox[{"this", ",", " ", 
    RowBox[{"even", " ", "simplified"}], ",", " ", 
    RowBox[{
    "is", " ", "not", " ", "particularly", " ", "englightening", " ", "from", 
     " ", "here"}]}], "*)"}]}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.740402853903785*^9, 3.740402870404729*^9}, {
  3.740402913309183*^9, 3.7404029781508913`*^9}, {3.747395055588911*^9, 
  3.747395070237749*^9}, {3.7473951213876743`*^9, 3.747395127622031*^9}, {
