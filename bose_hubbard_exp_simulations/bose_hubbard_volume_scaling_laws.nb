     RowBox[{
      RowBox[{
       RowBox[{"pn2", "[", 
        RowBox[{"n", ",", "L"}], "]"}], 
       RowBox[{"Log", "[", 
        RowBox[{"pn2", "[", 
         RowBox[{"n", ",", "L"}], "]"}], "]"}]}], "\[IndentingNewLine]", ",", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
